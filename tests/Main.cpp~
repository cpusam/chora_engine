#include "../src/Chora.hpp"

int main(){
	SDL_Init(SDL_INIT_VIDEO);
	SDL_Window *window = SDL_CreateWindow("Textures", SDL_WINDOWPOS_CENTERED, SDL_WINDOWPOS_CENTERED, 400, 400, 0);
	SDL_Renderer *renderer = SDL_CreateRenderer(window, -1, SDL_RENDERER_ACCELERATED);
	SDL_Event event;
	
	CTextureManager::instance()->add_texture("background.png",renderer);
	
	CTextureManager::instance()->destroy();
	
	SDL_Quit();
	
	return 0;
}
